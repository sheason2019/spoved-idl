import { Pagination } from "./common.omi";

struct DeployOrder {
  int id;

  int projectId;
  string image;
  string versoin;
  int createAt;
  string operator;

  int compileOrderId;

  int statusCode;
}

struct GetDeployOrdersPayload {
  int projectId;
  int page;
  int pageSize;
}

struct GetDeployOrdersResponse {
  repeated DeployOrder records;
  Pagination pagination;
}

service DeployApi {
  GetCompileOrdersResponse GetCompileOrders(GetCompileOrdersPayload payload);
}