import { Pagination } from "./common.omi";

struct CompileRecord {
  int id;

  int projectId;
  string image;
  string version;
  int createAt;
  string operator;
  string branch;

  string output;
  int statusCode;
}

struct GetCompileRecordResponse {
  Pagination pagination;
  CompileRecord records;
}

service CompileApi {
  CompileRecord PostCompile(CompileRecord payload);
  GetCompileRecordResponse GetCompileRecords(Pagination pagination);
}